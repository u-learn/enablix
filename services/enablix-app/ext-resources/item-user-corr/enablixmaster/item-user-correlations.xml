<?xml version="1.0" encoding="utf-8"?>
<itemUserCorrelationRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/item-correlation-rule.xsd">
	<itemUserCorrelationRule id="itemUserRule1" name="Solution sheet - Matching user region">
		<triggerItem qualifiedId="solution.solutionSheet">
		</triggerItem>
		<relatedUsers>
			<relatedUserPath>
				<pathItem qualifiedId="businessRole">
					<filterCriteria>
						<filter>
							<attributeId>role</attributeId>
							<constant>Sales</constant>
						</filter>
					</filterCriteria>
					<relatedUser>
						<filterCriteria>
							<filter>
								<attributeId>userSolArea</attributeId>
								<parentAttribute parentQId="solution">solArea</parentAttribute>
							</filter>
							<filter>
								<attributeId>role</attributeId>
								<parentAttribute>identity</parentAttribute>
							</filter>
						</filterCriteria>
						<tags>
							<tag>SALES_PERSON</tag>
						</tags>
					</relatedUser>
				</pathItem>
			</relatedUserPath>
		</relatedUsers>
	</itemUserCorrelationRule>
</itemUserCorrelationRules>
