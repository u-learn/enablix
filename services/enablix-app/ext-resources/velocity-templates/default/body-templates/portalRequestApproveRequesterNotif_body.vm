#@mail_template(${obj.contentRequest.objectRef.contentTitle})

	#spacer_double_row('20' '15')
											
	#mail_opening(${obj.recipientUser.profile.name})
	
	#spacer_row('15')
	
	#if(${obj.loggedInUser.profile.name}) ${obj.loggedInUser.profile.name}#else Someone #end has approved your request to #if($obj.contentRequest.objectRef.addRequest) add #else update #end 
	
