<?xml version="1.0" encoding="utf-8"?>
<itemCorrelationRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/item-correlation-rule.xsd">
	<itemCorrelationRule id="rule11" name="Product Correlation">
		<triggerItem qualifiedId="salesPlay">
		</triggerItem>
		<relatedItems>
			<relatedItem qualifiedId="product">
				<filterCriteria>
					<filter>
						<attributeId>identity</attributeId>
						<parentAttribute>products</parentAttribute>
					</filter>
				</filterCriteria>
				<recordAsRelated>true</recordAsRelated>
				<tags>
					<tag>LINKED_PROD</tag>
				</tags>
			</relatedItem>
		</relatedItems>
	</itemCorrelationRule>
	<itemCorrelationRule id="rule12" name="Solution Correlation">
		<triggerItem qualifiedId="salesPlay">
		</triggerItem>
		<relatedItems>
			<relatedItem qualifiedId="solution">
				<filterCriteria>
					<filter>
						<attributeId>identity</attributeId>
						<parentAttribute>solutions</parentAttribute>
					</filter>
				</filterCriteria>
				<recordAsRelated>true</recordAsRelated>
				<tags>
					<tag>LINKED_SOL</tag>
				</tags>
			</relatedItem>
		</relatedItems>
	</itemCorrelationRule>	
	<itemCorrelationRule id="rule13" name="Competitor Correlation">
		<triggerItem qualifiedId="salesPlay">
		</triggerItem>
		<relatedItems>
			<relatedItem qualifiedId="competitor">
				<filterCriteria>
					<filter>
						<attributeId>identity</attributeId>
						<parentAttribute>competitors</parentAttribute>
					</filter>
				</filterCriteria>
				<recordAsRelated>true</recordAsRelated>
