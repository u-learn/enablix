	</itemCorrelationRule>	
	<itemCorrelationRule id="rule14" name="Product Introduction Correlation">
		<triggerItem qualifiedId="salesPlay">
		</triggerItem>
		<relatedItems>
			<relatedItem qualifiedId="product">
				<filterCriteria>
					<filter>
						<attributeId>shortName</attributeId>
						<constant>miRevenue</constant>
					</filter>
				</filterCriteria>
				<recordAsRelated>true</recordAsRelated>
				<tags>
					<tag>INTRO_PROD</tag>
				</tags>
				<relatedItems>
					<relatedItem qualifiedId="battlecard">
						<filterCriteria>
							<filter>
								<attributeId>products</attributeId>
								<parentAttribute>identity</parentAttribute>
							</filter>
						</filterCriteria>
						<recordAsRelated>true</recordAsRelated>
						<tags>
							<tag>PROD_BATTLECARD</tag>
						</tags>
					</relatedItem>				
					<relatedItem qualifiedId="salesDeck">
						<filterCriteria>
							<filter>
								<attributeId>products</attributeId>
								<parentAttribute>identity</parentAttribute>
							</filter>
						</filterCriteria>
						<recordAsRelated>true</recordAsRelated>
						<tags>
							<tag>PROD_SALES_DECK</tag>
						</tags>
					</relatedItem>								
					<relatedItem qualifiedId="whitepaper">
						<filterCriteria>
							<filter>
								<attributeId>products</attributeId>
								<parentAttribute>identity</parentAttribute>
							</filter>
						</filterCriteria>
						<recordAsRelated>true</recordAsRelated>
						<tags>
							<tag>PROD_WP</tag>
						</tags>
					</relatedItem>	
					<relatedItem qualifiedId="caseStudy">
						<filterCriteria>
							<filter>
								<attributeId>products</attributeId>
								<parentAttribute>identity</parentAttribute>
							</filter>
						</filterCriteria>
						<recordAsRelated>true</recordAsRelated>
						<tags>
							<tag>PROD_CS</tag>
						</tags>
					</relatedItem>
				</relatedItems>								
			</relatedItem>
		</relatedItems>
	</itemCorrelationRule>		
</itemCorrelationRules>